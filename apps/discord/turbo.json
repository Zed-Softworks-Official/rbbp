{
    "$schema": "https://turbo.build/schema.json",
    "extends": ["//"],
    "tasks": {
        "deploy-commands": {
            "outputs": []
        },
        "dev": {
            "outputs": ["dist/**"],
            "dependsOn": ["deploy-commands"],
            "persistent": true
        },
        "start": {
            "dependsOn": ["build"],
            "persistent": true
        }
    }
}
