{
    "$schema": "https://turbo.build/schema.json",
    "extends": ["//"],
    "tasks": {
        "deploy-commands": {
            "outputs": []
        },
        "dev": {
            "outputs": ["dist/**"],
            "dependsOn": ["deploy-commands"],
            "persistent": true
        },
        "build-bot": {
            "outputs": ["dist/**"]
        },
        "start-bot": {
            "cache": false,
            "dependsOn": ["build-bot"],
            "persistent": true
        }
    }
}
